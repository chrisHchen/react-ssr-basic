{
  "name": "react-ssr-basic",
  "version": "0.1.0",
  "description": "a simple ssr example with the most basic tools",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "chrishchen"
  },
  "scripts": {
    "start": "rm -rf ./build/* && nodemon --watch server --watch src --watch util --exec babel-node ./server/server.js",
    "build": "rm -rf ./build/* && babel-node build.js",
    "deploy": "cross-env NODE_ENV=production babel-node app.js"
  },
  "private": true,
