    "@babel/register": "^7.0.0-beta.40",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.2.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^20.0.4",
    "babel-loader": "8.0.0-beta.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^3.19.0",
